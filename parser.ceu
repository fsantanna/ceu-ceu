export [parser]
{
    func ex-prim () {
        ifs {
            tk-fetch(:Tk.Key.val) {
                val tk = tks.pre
                val id :Tk.Id = tk-force(:Tk.Id)
                val op :Tk.Op = tk-fetch(:Tk.Op, "=")
                val src = op and ex-prim()
                :Ex.Dcl [tk, id.str, src]
            }
            ;;else -> :Ex.TODO []
        }
    }

    func parser () {
        ex-prim()
    }
}
