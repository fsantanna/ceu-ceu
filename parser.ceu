export [parser]
{
    func ex-prim () {
        ifs {
            tk-fetch(:Tk.Key.val) {
                val tk = tk-pre
                val id :Tk.Id = tk-force(:Tk.Id)
                val op :Tk.Op = tk-fetch(:Tk.Op, "=")
                :Ex.Dcl [tk, id.str, op and ex-prim()]
            }
            else -> :Ex.TODO []
        }
    }

    func parser () {
        ex-prim()
    }
}
